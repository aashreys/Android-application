<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vivian.capstone_test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/BlendTheme"
        android:name=".BlendApplication">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EventsActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".PingActivity"></activity>
        <activity
            android:name=".CreateEventActivity"
            android:label="@string/title_activity_create_events"
            android:theme="@style/BlendTheme"></activity>
    </application>

</manifest>